{
  "name": "dat-land",
  "version": "1.2.3",
  "description": "An online place for dats.",
  "main": "index.js",
  "scripts": {
    "build:licenses": "brlicenses -f full index.js > LICENSES",
    "build:js": "browserify -p [ browserify-header --file header.js ] index.js > bundle.js",
    "build:css": "npm-css public/src/css/base.css -o public/dist/css/base.css",
    "build": "npm run build:js && npm run build:css && npm run build:licenses",
    "watch": "watchify index.js -o bundle.js",
    "start": "npm run watch & http-server .",
    "deploy": "gh-pages-deploy",
    "test": "standard",
    "lint": "standard"
  },
  "author": "Karissa McKelvey <karissa@karissamck.com> (http://karissamck.com/)",
  "license": "MIT",
  "devDependencies": {
    "brfs": "^1.4.3",
    "browserify": "^12.0.0",
    "browserify-header": "^0.9.2",
    "browserify-licenses": "^0.1.1",
    "choppa": "^1.0.2",
    "concat-stream": "^1.5.1",
    "dat-design": "^1.1.0",
    "drag-drop": "^2.11.0",
    "filereader-stream": "^1.0.0",
    "gh-pages-deploy": "^0.4.2",
    "http-server": "^0.9.0",
    "hyperdrive": "^6.2.1",
    "hyperdrive-archive-swarm": "^3.0.1",
    "hyperdrive-ui": "^2.0.0",
    "intro.js": "^2.1.0",
    "level-browserify": "^1.1.0",
    "memdb": "^1.3.1",
    "minidux": "^1.0.1",
    "npm-css": "^0.2.3",
    "pretty-bytes": "^3.0.1",
    "speedometer": "^1.0.0",
    "standard": "^7.1.2",
    "watchify": "~3.6.0",
    "wzrd": "^1.3.1"
  },
  "gh-pages-deploy": {
    "prep": [
      "build"
    ],
    "noprompt": false
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/karissa/hyperdrive-ui.git"
  },
  "bugs": {
    "url": "https://github.com/karissa/hyperdrive-ui/issues"
  }
}
